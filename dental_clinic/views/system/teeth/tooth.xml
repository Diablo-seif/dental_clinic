<odoo>
  <data>

        <record id="view_tooth_tooth_form" model="ir.ui.view">
            <field name="name">tooth_tooth_form</field>
            <field name="model">tooth.tooth</field>
            <field name="arch" type="xml">
                <form string="tooth_form">
                    <sheet>
                        <group>
                        <field name="name"/>
                        <field name="title"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_tooth_tooth_tree" model="ir.ui.view">
          <field name="name">tooth.tooth.tree</field>
          <field name="model">tooth.tooth</field>
          <field name="arch" type="xml">
              <tree>
                        <field name="name"/>
                        <field name="title"/>
              </tree>
          </field>
        </record>

        <record id="action_tooth_tooth" model="ir.actions.act_window">
          <field name="name">Teeth</field>
          <field name="res_model">tooth.tooth</field>
          <field name="view_mode">tree,form</field>
        </record>
  </data>
</odoo>